CHOST="x86_64-pc-linux-gnu"
CFLAGS="-march=skylake -O2 -pipe"
CXXFLAGS="${CFLAGS}"
 
# NOTE: This stage was built with the bindist Use flag enabled
PORTDIR="/usr/portage"
DISTDIR="/usr/portage/distfiles"
PKGDIR="/usr/portage/packages"
 
# This sets the language of build output to English.
# Please keep this setting intact when reporting bugs.
LC_MESSAGES=C
 
VIDEO_CARDS="nvidia intel i965"
 
MAKEOPTS="-j9"
 
GENTOO_MIRRORS="http://mirror.eu.oneandone.net/linux/distributions/gentoo/gentoo/"
 
USE="-cups -qt4 -qt5 -kde -nouveau -sysvinit -openrc -networkmanager X acl alsa amd64 bzip2 cli dri gtk gnome jpeg2k mount multilib ntp nvidia perl raw ssl static-libs sudo tools udisks udev uvesafb vdpau vim"
 
ABI_X86="32*"
